package tri;

public class TriBulle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	
	
	public static void triBulle(int tab[], int len) {
		
		if(len > 0) {
		for (int i = 0; i < len-1; i++) {
			if(tab[i] > tab[i+1]) {
				int temp = tab[i];
				tab[i] = tab[i+1];
				tab[i+1] = temp;
			}
		}
	}
	}
		

}
