package tri;

public class TriSelection {

	public static void main(String[] args) {
		int[] t = {2, 4, 9, 5, 7, 3, 6, 1};
		
		triSelection(t);
		
		for (int i = 0; i < t.length; i++) {
			System.out.println(t[i]);
		}

	}
	
	
	public static void triSelection(int[] tab) {
		
		for (int i = 0; i < tab.length; i++) {
			
		}

	}

}
