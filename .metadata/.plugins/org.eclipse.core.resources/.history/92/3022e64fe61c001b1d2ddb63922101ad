package fr.eql.ai108.tp.biblioth√®que;

public class Livre {

	String titre;
	String auteur;
	int isbns;
	Categorie categorie;
	public Livre(String titre, String auteur, int isbn, Categorie categorie) {
		super();
		this.titre = titre;
		this.auteur = auteur;
		this.isbn = isbn;
		this.categorie = categorie;
	}
	public String getTitre() {
		return titre;
	}
	public void setTitre(String titre) {
		this.titre = titre;
	}
	public String getAuteur() {
		return auteur;
	}
	public void setAuteur(String auteur) {
		this.auteur = auteur;
	}
	public int getBisbn() {
		return isbn;
	}
	public void setBisbn(int bisbn) {
		this.bisbn = isbn;
	}
	public Categorie getCategorie() {
		return categorie;
	}
	public void setCategorie(Categorie categorie) {
		this.categorie = categorie;
	}
	
	
}
